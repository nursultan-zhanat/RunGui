local player = game.Players.LocalPlayer
local StarterGui = player:WaitForChild("PlayerGui")
local speeed = player.Character.Humanoid.WalkSpeed

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = StarterGui
ScreenGui.Name = "SpeedGui"
local Frame =  Instance.new("Frame")
Frame.Parent = ScreenGui
Frame.Name = "SpeedFrame"
Frame.Position = UDim2.new(0.791, 0,0.334, 0)
Frame.Size = UDim2.new(0, 200,0, 85)
Frame.BorderSizePixel = 1
Frame.BackgroundColor3 = Color3.fromRGB(14, 127, 6)
local TextLabel = Instance.new("TextLabel")
TextLabel.Parent = Frame
TextLabel.Name = "SpeedText"
TextLabel.Text = "Speed"
TextLabel.TextSize = 20
TextLabel.BorderSizePixel = 1
TextLabel.TextStrokeTransparency = 0.83
TextLabel.TextColor3 = Color3.fromRGB(0, 145, 255)
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
TextLabel.Position = UDim2.new(0, 0,0, 0)
TextLabel.Size = UDim2.new(0, 200,0, 23)
local speed = Instance.new("TextLabel")
speed.Parent = Frame 
speed.Name = "speed"
speed.BackgroundTransparency = 1
speed.Position = UDim2.new(0.3, 0,0.459, 0)
speed.Size = UDim2.new(0, 79,0, 30)
speed.Text = speeed
speed.TextScaled = true
local plus = Instance.new("TextButton")
plus.Parent = Frame 
plus.Name = "+"
plus.BackgroundColor3 = Color3.fromRGB(255, 7, 243)
plus.BorderSizePixel = 1
plus.Position = UDim2.new(0.1, 0,0.41, 0)
plus.Size = UDim2.new(0, 40,0, 40)
plus.Text = "+"
plus.TextScaled = true
plus.TextStrokeTransparency = 1
plus.MouseButton1Click:Connect(function()
	player.Character.Humanoid.WalkSpeed = player.Character.Humanoid.WalkSpeed + 1
	speed.Text = player.Character.Humanoid.WalkSpeed
end)
local minus = Instance.new("TextButton")
minus.Parent = Frame 
minus.Name = "-"
minus.BackgroundColor3 = Color3.fromRGB(255, 7, 243)
minus.BorderSizePixel = 1
minus.Position = UDim2.new(0.7, 0,0.41, 0)
minus.Size = UDim2.new(0, 40,0, 40)
minus.Text = "-"
minus.TextScaled = true
minus.TextStrokeTransparency = 1
minus.MouseButton1Click:Connect(function()
	player.Character.Humanoid.WalkSpeed = player.Character.Humanoid.WalkSpeed - 1
	speed.Text = player.Character.Humanoid.WalkSpeed
end)
